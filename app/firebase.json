{
    "firestore": {
        "rules": "firestore.rules"
    },
    "functions": {
        "source": "../functions",
        "predeploy": [
            "npm --prefix ../functions run build"
        ]
    },
    "hosting": {
        "public": "build/web",
        "ignore": [
            "firebase.json",
            "**/.*",
            "**/node_modules/**"
        ],
        "rewrites": [
            {
                "source": "**",
                "destination": "/index.html"
            }
        ],
        "headers": [
            {
                "source": "**/*.@(js|css|html|json)",
                "headers": [
                    {
                        "key": "Cache-Control",
                        "value": "no-cache, no-store, must-revalidate, max-age=0"
                    },
                    {
                        "key": "Pragma",
                        "value": "no-cache"
                    },
                    {
                        "key": "Expires",
                        "value": "0"
                    }
                ]
            },
            {
                "source": "**/index.html",
                "headers": [
                    {
                        "key": "Cache-Control",
                        "value": "no-cache, no-store, must-revalidate, max-age=0"
                    },
                    {
                        "key": "Pragma",
                        "value": "no-cache"
                    },
                    {
                        "key": "Expires",
                        "value": "0"
                    }
                ]
            },
            {
                "source": "**/main.dart.js",
                "headers": [
                    {
                        "key": "Cache-Control",
                        "value": "no-cache, no-store, must-revalidate, max-age=0"
                    }
                ]
            },
            {
                "source": "**/flutter_service_worker.js",
                "headers": [
                    {
                        "key": "Cache-Control",
                        "value": "no-cache, no-store, must-revalidate, max-age=0"
                    }
                ]
            },
            {
                "source": "**/flutter_bootstrap.js",
                "headers": [
                    {
                        "key": "Cache-Control",
                        "value": "no-cache, no-store, must-revalidate, max-age=0"
                    }
                ]
            },
            {
                "source": "**/*.@(jpg|jpeg|gif|png|svg|ico|woff|woff2|ttf|eot)",
                "headers": [
                    {
                        "key": "Cache-Control",
                        "value": "public, max-age=31536000, immutable"
                    }
                ]
            }
        ]
    },
    "flutter": {
        "platforms": {
            "android": {
                "default": {
                    "projectId": "poker-fa33a",
                    "appId": "1:790242418126:android:4c9cc0a214c44672109f9f",
                    "fileOutput": "android/app/google-services.json"
                }
            },
            "dart": {
                "lib/firebase_options.dart": {
                    "projectId": "poker-fa33a",
                    "configurations": {
                        "android": "1:790242418126:android:4c9cc0a214c44672109f9f",
                        "ios": "1:790242418126:ios:bab3aad38cd1eef6109f9f",
                        "macos": "1:790242418126:ios:bab3aad38cd1eef6109f9f",
                        "web": "1:790242418126:web:58351530f85c60bf109f9f",
                        "windows": "1:790242418126:web:b13b4ffd7b38e015109f9f"
                    }
                }
            }
        }
    }
}